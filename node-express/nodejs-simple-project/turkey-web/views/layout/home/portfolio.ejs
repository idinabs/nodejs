<div class="row">
  <div class="span12">
    <h4 class="heading" style="margin-top: 100px; margin-left: 48px; border-radius: 40px; color: white"><b>Tentang</b> <strong
        style="color: #b39107">Turki???</strong></h4>
    <div class="row">
      <section id="projects" style="margin-left: 90px">
        <ul id="thumbs" class="portfolio">
          <!-- Item Project and Filter Name -->
          <% data.forEach(function(data) { %>
            <li class="item-thumbs span4 design" style="border-radius: 60px; width: 19rem;  text-align: center;" data-id="id-0" data-type="web">
              <a class="hover-wrap fancybox" data-fancybox-group="gallery" title="The City" href="<%= data.image %> ">
                <span class="overlay-img"></span>
                <span class="overlay-img-thumb font-icon-plus"></span>
              </a>
              <img src="<%= data.image %> ">
            </li>
            <!-- End Item Project -->
            <% }) %>
        </ul>
      </section>
    </div>
  </div>
</div>